@model IEnumerable<mvc.Models.Sex>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SexeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SexeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SexeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SexeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SexeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
